  },
  "devDependencies": {
    "fly": "^2.0.5",
    "fly-clear": "^1.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.2.2",
    "xo": "*"
  },
  "engines": {
    "node": ">= 4.6"
  },
  "xo": {
